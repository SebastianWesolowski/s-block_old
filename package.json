{
  "name": "s-block",
  "version": "0.0.0-development",
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "build": "run-s compile rollup:build",
    "rollup:build": "rollup -c",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "test:changed": "jest --onlyChanged",
    "eject": "react-scripts eject",
    "storybook": "NODE_PATH=src start-storybook -p 6006 -c .storybook -s ./public",
    "build-storybook": "build-storybook -s public",
    "compile": "run-s compile:clear compile:buildJS compile:alias compile:copy",
    "compile:clear": "npm run build:clear",
    "compile:buildJS": "tsc --project ./tsconfig.compile.json --declaration",
    "compile:alias": "tsc-alias -p ./tsconfig.compile.json",
    "compile:copy": "copyfiles -u 1 \"./src/**/*.+(png|svg|scss)\"  distTS",
    "build:clear": "rimraf distTS",
    "semantic-release": "semantic-release --branches main",
    "lint": "run-s format:check lint:parallel",
    "lint:parallel": "run-p lint:check style:check",
    "lint:check": "eslint \"./src/**/*.+(js|jsx|ts|tsx)\" --cache",
    "lint:check:staged": "eslint -ext .js, .jsx, .ts, .tsx --cache",
    "lint:fix": "run-s format \"lint:check -- --fix\"",
    "format": "run-s format:check format:write",
    "format:check": "prettier --check \"./**/*.+(js|jsx|ts|tsx|json)\" --ignore-unknown --json-recursive-sort",
    "format:write": "prettier --write \"./**/*.+(js|jsx|ts|tsx|json)\" --ignore-unknown --json-recursive-sort",
    "format:check:staged": "prettier --check --ignore-unknown --json-recursive-sort",
    "style:check": "stylelint \"src/**/*.+(scss|css)\" --cache",
    "style:check:staged": "stylelint \"*.{scss,css}\" --cache",
    "style:fix": "run-s format \"style:check -- --fix\"",
    "acp": "git add . && npm run commit && git push",
    "commit": "git-cz",
    "new:component": "hygen new component --section components"
  },
  "dependencies": {
    "@emotion/core": "^10.3.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.7",
    "@mui/styled-engine-sc": "^5.1.0",
    "@mui/styles": "^5.3.0",
    "@mui/system": "^5.3.0",
    "@storybook/react": "^6.5.0-alpha.5",
    "@types/styled-components": "^5.1.21",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "enzyme": "^3.11.0",
    "hygen": "^6.1.0",
    "next": "^12.0.9",
    "s-brand": "^1.3.2",
    "storybook-addon-mui-mode": "^0.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@netlify/plugin-nextjs": "^4.2.4",
    "@react-theming/storybook-addon": "^1.1.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-a11y": "^6.5.0-alpha.5",
    "@storybook/addon-actions": "^6.5.0-alpha.5",
    "@storybook/addon-essentials": "^6.5.0-alpha.5",
    "@storybook/addon-links": "^6.5.0-alpha.5",
    "@storybook/addons": "^6.5.0-alpha.5",
    "@storybook/builder-webpack5": "^6.5.0-alpha.5",
    "@storybook/manager-webpack5": "^6.5.0-alpha.5",
    "@storybook/node-logger": "^6.5.0-alpha.5",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/storybook-deployer": "^2.8.10",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/dom": "^7.21.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.15",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.0.0",
    "copyfiles": "^2.4.1",
    "crypto-browserify": "^3.12.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-storybook": "^0.5.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-refresh": "^0.11.0",
    "react-scripts": "5.0.0",
    "rollup": "^2.61.1",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.45.1",
    "semantic-release": "^18.0.1",
    "storybook-addon-outline": "^1.4.2",
    "storybook-readme": "^5.0.9",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tsc-alias": "^1.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "private": false,
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "description": "S-Block React Component Library",
  "author": "Sebastian Wesolowski",
  "homepage": "https://github.com/SebastianWesolowski/s-block",
  "bugs": {
    "url": "https://github.com/SebastianWesolowski/s-block/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWesolowski/s-block.git"
  },
  "transform": {
    "\\.tsx$": "<rootDir>/node_modules/babel-jest"
  }
}
